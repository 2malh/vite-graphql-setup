{
  "name": "vite-graphql-setup",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production vite build",
    "postinstall": "yarn patch:apollo && yarn fix:vue-demi",
    "patch:apollo": "node scripts/vue-apollo-patch.js",
    "fix:vue-demi": "node node_modules/.bin/vue-demi-fix",
    "codegen": "graphql-codegen --config codegen.yml",
    "format": "prettier --write **/*.{js,ts,vue,json}",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@vue/apollo-composable": "^4.0.0-alpha.12",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "vue": "^3.0.4",
    "vue-router": "^4.0.1"
  },
  "devDependencies": {
    "@apollo/link-schema": "^2.0.0-beta.3",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-tools/mock": "^7.0.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/vue": "^6.3.1",
    "@types/jest": "^26.0.19",
    "@vue/compiler-sfc": "^3.0.4",
    "autoprefixer": "^9",
    "cross-fetch": "^3.0.6",
    "deepmerge": "^4.2.2",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.3",
    "postcss": "^7",
    "prettier": "^2.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "vite": "^1.0.0-rc.13",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
